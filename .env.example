# Rename this file to .env and fill in your real keys

PORT=3000

# Public key (safe to expose)
WOMPI_PUBLIC_KEY=pub_prod_YOUR_PUBLIC_KEY

# Private key (server only!)
WOMPI_PRIVATE_KEY=prv_prod_YOUR_PRIVATE_KEY

# Webhook secret for signature validation
WOMPI_WEBHOOK_SECRET=prod_events_YOUR_WEBHOOK_SECRET
